services:
  basket.api:
    build:
      context: E:\MyFinalProject\WebOrder
      dockerfile: E:\MyFinalProject\WebOrder\Services\Basket\Basket.API\Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Basket.API
      target: base
    container_name: Basket.API
    depends_on:
      basketdata:
        condition: service_started
      identity.api:
        condition: service_started
      rabbitmq:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: http://0.0.0.0:80
      ApplicationInsights__InstrumentationKey: ""
      AzureServiceBusEnabled: "False"
      ConnectionString: basketdata
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      EventBusConnection: rabbitmq
      EventBusPassword: ""
      EventBusUserName: ""
      GRPC_PORT: "81"
      IdentityUrlExternal: http://:5105
      NUGET_FALLBACK_PACKAGES: ""
      OrchestratorType: ""
      PATH_BASE: /basket-api
      PORT: "80"
      UseLoadTest: "False"
      identityUrl: http://identity.api
    image: eshop/basket.api:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/Web.Order.Service.Basket.BasketAPI.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$(pidof
        dotnet); then kill $PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: 5103
      protocol: tcp
    - mode: ingress
      target: 81
      published: 9103
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: E:\MyFinalProject\WebOrder\Services\Basket\Basket.API
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: E:\MyFinalProject\WebOrder
      target: /src
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Minh Vu\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Minh Vu\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Minh Vu\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Minh Vu\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
  basketdata:
    image: redis:alpine
    networks:
      default: null
    ports:
    - mode: ingress
      target: 6379
      published: 6379
      protocol: tcp
  identity.api:
    build:
      context: E:\MyFinalProject\WebOrder
