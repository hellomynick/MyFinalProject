services:
  identity.api:
    build:
      context: E:\MyFinalProject\WebOrder
      dockerfile: E:\MyFinalProject\WebOrder\Identity.API\Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Identity.API
      target: base
    container_name: Identity.API
    depends_on:
      sqldata:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: http://0.0.0.0:80
      ApplicationInsights__InstrumentationKey: ""
      BasketApiClient: http://:5103
      ConnectionString: Server=sqldata;Database=WebOrder.Service.IdentityDb;User Id=sa;Password=Admin@123
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      MobileShoppingAggClient: http://:5120
      MvcClient: http://:5100
      NUGET_FALLBACK_PACKAGES: ""
      OrchestratorType: ""
      OrderingApiClient: http://:5102
      SpaClient: http://:5104
      UseCustomizationData: "True"
      WebShoppingAggClient: http://:5121
      WebhooksApiClient: http://:5113
      WebhooksWebClient: http://:5114
      XamarinCallback: http://:5105/xamarincallback
    image: eshop/identityapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/Identity.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$(pidof
        dotnet); then kill $PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: 5105
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: E:\MyFinalProject\WebOrder\Identity.API
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: E:\MyFinalProject\WebOrder
      target: /src
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Minh Vu\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Minh Vu\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Minh Vu\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Minh Vu\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
  seq:
    environment:
      ACCEPT_EULA: "Y"
    image: datalust/seq:latest
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: 5340
      protocol: tcp
  sqldata:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Admin@123
    image: mcr.microsoft.com/mssql/server:2019-latest
    networks:
      default: null
    ports:
    - mode: ingress
      target: 1433
      published: 5433
      protocol: tcp
networks:
  default:
    name: dockercompose10855430136696232203_default